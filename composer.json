{
  "name": "athlon1600/php-cidr-range-optimizer",
  "description": "Given a list of IP ranges, merge them into the smallest possible list of CIDRs.",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "cidr",
    "ip ranges",
    "cidr optimizer",
    "subnet optimizer",
    "network range optimizer",
    "cidr merge"
  ],
  "require": {
    "php": ">=7.4.0"
  },
  "require-dev": {
    "phpunit/phpunit": ">=8.0",
    "phpstan/phpstan": "^1.11"
  },
  "autoload": {
    "psr-4": {
      "ProxyNova\\RangeOptimizer\\": "src/"
    },
    "files": [
      "src/global.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "ProxyNova\\RangeOptimizer\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit tests",
    "static": "./vendor/bin/phpstan analyse src --level 6 --no-progress"
  }
}
